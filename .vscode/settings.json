{
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "python.linting.pylintArgs": ["--disable=C0111"],
  "python.formatting.provider": "autopep8",
  "python.autoComplete.extraPaths": [
    "${workspaceFolder}/__pypackages__/3.10/lib"
  ],
  "python.analysis.extraPaths": ["${workspaceFolder}/__pypackages__/3.10/lib"],
  "terminal.integrated.defaultProfile.linux": "zsh",
  "workbench.colorTheme": "Dracula",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "Dracula",
  "workbench.preferredHighContrastColorTheme": "Dracula",
  "workbench.preferredHighContrastLightColorTheme": "Dracula",
  "workbench.preferredLightColorTheme": "Dracula",
  "window.autoDetectColorScheme": true,
  "editor.semanticTokenColorCustomizations": {
    "[Dracula]": { "enabled": true }
  },
  "editor.formatOnSave": true,
  "python.testing.pytestArgs": ["src", "--ignore", "__pypackages__", "-s"],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true
  //"python.testing.unittestArgs": ["-v", "-s", "./src", "-p", "test_*.py"],
  //"python.testing.pytestEnabled": false,
  //"python.testing.unittestEnabled": true
}
